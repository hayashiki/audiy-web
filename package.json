{
  "name": "audiy-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "jest": "jest",
    "export": "next build && next export",
    "fmt": "prettier --write 'src/**/*.{js,json,ts,tsx}'",
    "fmt:quick": "pretty-quick --staged",
    "lint": "eslint --ext js,ts,tsx .",
    "lint:fix": "eslint --ext js,ts,tsx . --fix",
    "generate": "graphql-codegen",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@apollo/react-hooks": "^4.0.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "clsx": "^1.1.1",
    "dotenv": "^10.0.0",
    "firebase": "^9.0.1",
    "formik": "^2.2.9",
    "graphql": "^15.5.1",
    "next": "11.0.1",
    "next-auth": "^3.27.3",
    "nprogress": "^0.2.0",
    "ramda": "^0.27.1",
    "react": "17.0.2",
    "react-compound-slider": "^3.3.1",
    "react-dom": "17.0.2",
    "react-infinite-scroller": "^1.2.4",
    "react-social-login-buttons": "^3.5.0",
    "types": "^0.1.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/typescript": "^1.22.3",
    "@graphql-codegen/typescript-operations": "^1.18.2",
    "@graphql-codegen/typescript-react-apollo": "^2.2.7",
    "@storybook/addon-actions": "^6.3.0",
    "@storybook/addon-backgrounds": "^6.3.0",
    "@storybook/addon-docs": "^6.3.0",
    "@storybook/addon-knobs": "^6.3.0",
    "@storybook/addon-links": "^6.3.0",
    "@storybook/addon-storysource": "^6.3.0",
    "@storybook/addon-viewport": "^6.3.0",
    "@storybook/addons": "^6.3.0",
    "@storybook/react": "^6.3.0",
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.2.1",
    "@trivago/prettier-plugin-sort-imports": "^2.0.4",
    "@types/gapi": "^0.0.40",
    "@types/gapi.auth2": "^0.0.55",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4",
    "@types/nprogress": "^0.2.0",
    "@types/ramda": "^0.27.44",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-infinite-scroller": "^1.2.2",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "apollo-cache-inmemory": "^1.6.6",
    "eslint": "^7.29.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "graphql-anywhere": "^4.2.7",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  },
  "main": "index.js",
  "author": "hayashiki <masayuki@hayashida.dev>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/src/**/*.test.ts"
    ],
    "watchPathIgnorePatterns": [],
    "collectCoverageFrom": [
      "src/**/*.{ts,js,jsx}",
      "!src/cli.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
